auto lo
iface lo inet loopback

auto eth0
iface eth0 inet dhcp
    hostname alpine38.localdomain

auto eth2
iface eth2 inet static
    address 192.168.10.1
    netmask 255.255.255.0
    gateway 10.0.2.2

auto eth3
iface eth3 inet static
    address 10.5.92.1
    netmask 255.255.255.0
    gateway 10.0.2.2
